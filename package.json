{
  "name": "terra-framework",
  "version": "1.0.0",
  "description": "MERN stack Restful API Framework",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd Develop/server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build",
    "render-build": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peteCodes4u/terra-framework.git"
  },
  "keywords": [
    "Mongo",
    "React",
    "Express",
    "JS",
    "Node",
    "JS",
    "REST",
    "API"
  ],
  "author": "peteCodes4u",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "bugs": {
    "url": "https://github.com/peteCodes4u/terra-framework/issues"
  },
  "homepage": "https://github.com/peteCodes4u/terra-framework#readme"
}
